{
  "name": "ui",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.tsx --format esm,cjs --dts --external react",
    "clean": "rm -rf dist",
    "dev": "tsup src/index.tsx --format esm,cjs --watch --dts --external react",
    "lint": "eslint \"src/**/*.ts*\"",
    "test": "jest"
  },
  "jest": {
    "preset": "jest-presets/jest/node",
    "testEnvironment": "jsdom"
  },
  "devDependencies": {
    "@types/jest": "29.5.3",
    "@types/react": "18.2.19",
    "@types/react-dom": "18.2.7",
    "eslint": "8.46.0",
    "jest": "29.6.2",
    "jest-environment-jsdom": "29.6.2",
    "jest-presets": "workspace:*",
    "react": "18.2.0",
    "tsconfig": "workspace:*",
    "tsup": "7.2.0",
    "typescript": "5.1.6"
  },
  "dependencies": {
    "react-dom": "18.2.0"
  }
}
