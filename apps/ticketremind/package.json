{
  "name": "ticketremind",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsup src/index.ts --format cjs",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "tsup src/index.ts --format cjs --watch --onSuccess \"node dist/index.js\"",
    "lint": "tsc --noEmit && eslint \"src/**/*.ts*\"",
    "start": "node dist/index.js",
    "test": "jest --detectOpenHandles"
  },
  "jest": {
    "preset": "jest-presets/jest/node"
  },
  "dependencies": {
    "@ai-flow/ai": "workspace:*",
    "@ai-flow/api": "workspace:*",
    "@ai-flow/core": "workspace:*",
    "@ai-flow/cron": "workspace:*",
    "@ai-flow/slack": "workspace:*",
    "@ai-flow/transforms": "workspace:*",
    "dotenv": "16.3.1"
  },
  "devDependencies": {
    "@types/jest": "29.5.3",
    "@types/node": "20.4.9",
    "@types/supertest": "2.0.12",
    "eslint": "8.46.0",
    "jest": "29.6.2",
    "jest-presets": "workspace:*",
    "supertest": "6.3.3",
    "tsconfig": "workspace:*",
    "tsup": "7.2.0",
    "typescript": "5.1.6"
  }
}